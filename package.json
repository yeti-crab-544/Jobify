{
  "name": "job-tracker",
  "version": "1.0.0",
  "description": "A tool for managing job applications.",
  "main": "index.js",
  "scripts": {
    "start": "nodemon server/server.js",
    "start:prod": "export NODE_ENV=production & node server/server.js",
    "start:dev": "export NODE_ENV=development & node server/server.js",
    "build": "export NODE_ENV=production && webpack",
    "dev": "export NODE_ENV=development && webpack serve --open --hot --progress --color",
    "dev:all": "concurrently \"cross-env npm run start:dev\" \"cross-env npm run dev\""
  },
  "nodemonConfig": {
    "ignore": [
      "build",
      "client"
    ]
  },
  "author": "Yeti Crab",
  "license": "ISC",
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.0",
    "body-parser": "^1.20.1",
    "browserify": "^17.0.0",
    "cors": "^2.8.5",
    "express": "^4.12.3",
    "mongoose": "^5.13.15",
    "node-fetch": "^2.3.0",
    "prop-types": "^15.6.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-dropdown-select": "^4.9.0",
    "react-hot-loader": "^4.6.3",
    "react-redux": "^8.0.5",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.2.0",
    "sass-loader": "^12.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "babel-loader": "^9.1.0",
    "concurrently": "^6.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.2",
    "eslint": "^7.17.0",
    "eslint-plugin-react": "^7.21.5",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "nodemon": "^2.0.7",
    "redux-devtools-extension": "^2.13.9",
    "sass": "^1.56.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-hot-middleware": "^2.24.3"
  }
}
